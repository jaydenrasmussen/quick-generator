{
  "name": "quick-generator",
  "author": "J. Rasmussen",
  "version": "3.0.0",
  "license": "MIT",
  "description": "A quick generator for node apps",
  "main": "app/app.js",
  "bin": "app/app.js",
  "pkg": {
    "assets": "{app, utils}/**/*"
  },
  "scripts": {
    "start": "node ./app/app.js",
    "test": "ava",
    "format": "prettier --single-quote --tab-width 4 --print-width 80 --write \"{,!(node_modules)/**/}*.js\"",
    "build": "pkg .",
    "build:win": "pkg . -t node8-win-x64",
    "build:lin": "pkg . -t node8-linux-x64",
    "build:mac": "pkg . -t node8-macos-x64"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "execa": "^0.8.0",
    "fs-extra": "^5.0.0",
    "lumberr": "^1.0.3",
    "meow": "^4.0.0",
    "moment": "^2.20.1"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "prettier": "^1.9.2",
    "test-console": "^1.1.0"
  }
}
